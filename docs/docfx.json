{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/windows/ai/breadcrumb/toc.json",
      "disable_contribution_on_translation_quality": ["MT"],
      "extendBreadcrumb": true,
      "ms.prod": "machine-learning"
    },
    "fileMetadata": {
      "ms.author":
      {
        "windows-ml/**.md": "quradic",
        "windows-vision-skills/**.md": "quradic"
      },
      "author":
      {
        "windows-ml/**.md": "QuinnRadich",
        "windows-vision-skills/**.md": "QuinnRadich"
      }
    },
    "template": [],
    "dest": "windows-ai-docs",
    "markdownEngineName": "markdig"
  }
}
